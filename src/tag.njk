---
layout: page
pagination:
  data: collections
  size: 1
  alias: tag
  filter:
    - all
    - post
permalink: tags/{{ tag }}/
eleventyComputed:
  title: "Tagged: {{ tag }}"
---

{% set taglist = collections[tag] %}

<ul>
  {% for item in taglist | selectattr('url') | reverse %}
    <li>
      <a href="{{ item.url }}">{{ item.data.title }}</a>
    </li>
  {% endfor %}
</ul>
